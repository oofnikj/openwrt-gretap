
config wifi-device 'radio0'
	option type 'mac80211'
	option phy 'phy0'
	option hwmode '11a'
	option htmode 'VHT80'
	option channel '36'
	option legacy_rates '0'

config wifi-iface 'wlan0'
	option device 'radio0'
	option mode 'ap'
	option ifname 'wlan0'
	option ssid 'sabich_5'
	option encryption 'psk2'
	option key 'xxxxxxxxxxxxx'
	option wds '1'
	option disassoc_low_ack '0'
	option network 'lan'

config wifi-iface 'trunk'
	option device 'radio0'
	option mode 'ap'
	option ifname 'wtun'
	option network 'wtun'
	option ssid 'trunk'
	option encryption 'psk2'
	option key 'xxxxxxxxxxxxx'
	option hidden '1'
	option disabled '0'
	option disassoc_low_ack '0'

